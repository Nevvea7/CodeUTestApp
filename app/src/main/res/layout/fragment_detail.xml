<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="app.nevvea.nomnom.DetailActivityFragment">

    <TextView  android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/detail_rest_name"
        />
    <TextView  android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/detail_rest_img"
        android:layout_below="@id/detail_rest_name"
        />
    <TextView  android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/detail_rest_phone"
        android:layout_below="@id/detail_rest_img"
        />
    <TextView  android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/detail_rest_addr"
        android:layout_below="@id/detail_rest_phone"
        />
    <TextView  android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/detail_rest_url"
        android:layout_below="@id/detail_rest_addr"
        />
</RelativeLayout>
